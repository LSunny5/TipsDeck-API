    "helmet": "^3.23.3",
    "knex": "^0.21.5",
    "morgan": "^1.10.0",
    "pg": "^8.3.2",
    "winston": "^3.3.3",
    "xss": "^1.0.8"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "cross-env": "^7.0.2",
    "mocha": "^7.2.0",
    "nodemon": "^2.0.3",
    "postgrator-cli": "^3.2.0",
    "supertest": "^4.0.2"
